{
  "name": "serverless-react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env IS_OFFLINE=1 concurrently --kill-others \"npm run start:devserver\" \"npm run start:offline\"",
    "start:devserver": "webpack serve --config webpack.browser.config.js --mode development",
    "start:offline": "sls offline start",
    "build:serverless": "sls package",
    "build:browser": "webpack --config webpack.browser.config.js --mode production",
    "deploy": "sls deploy",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "\\.(css|less|sass)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-stage-2": "^7.8.3",
    "@babel/preset-typescript": "^7.12.7",
    "@types/aws-lambda": "^8.10.71",
    "@types/babel-core": "^6.25.6",
    "@types/babel__core": "^7.1.12",
    "@types/babel__preset-env": "^7.9.1",
    "@types/concurrently": "^5.2.1",
    "@types/file-loader": "^4.2.0",
    "@types/jest": "^26.0.20",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/prettier": "^2.1.6",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/serverless": "^1.78.20",
    "@types/source-map-support": "^0.5.3",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-server": "^3.11.1",
    "aws-sdk": "^2.834.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.5",
    "null-loader": "^4.0.1",
    "prettier": "^2.2.1",
    "serverless": "^2.22.0",
    "serverless-export-env": "^1.4.1",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-s3-deploy": "^0.9.0",
    "serverless-webpack": "^5.3.5",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-stats-plugin": "^1.0.3"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always"
  }
}
