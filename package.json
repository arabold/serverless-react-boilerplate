{
  "name": "serverless-react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env IS_OFFLINE=1 concurrently --kill-others \"npm run start:devserver\" \"npm run start:offline\"",
    "start:devserver": "webpack serve --config webpack.browser.config.js --mode development",
    "start:offline": "sls offline start",
    "build:serverless": "sls package",
    "build:browser": "webpack --config webpack.browser.config.js --mode production",
    "deploy": "sls deploy",
    "test": "jest"
  },
  "dependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.8",
    "core-js": "^3.25.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.13.10",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-stage-2": "^7.8.3",
    "@babel/preset-typescript": "^7.18.6",
    "@types/aws-lambda": "^8.10.108",
    "@types/babel__core": "^7.1.19",
    "@types/babel__preset-env": "^7.9.2",
    "@types/babel-core": "^6.25.7",
    "@types/concurrently": "^6.4.0",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/core-js": "^2.5.5",
    "@types/file-loader": "^5.0.1",
    "@types/jest": "^29.2.0",
    "@types/mini-css-extract-plugin": "^2.5.0",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/serverless": "^3.12.8",
    "@types/source-map-support": "^0.5.6",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.7.1",
    "aws-sdk": "^2.1238.0",
    "babel-jest": "^29.2.1",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.5.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.1",
    "jest-module-name-mapper": "^0.1.5",
    "mini-css-extract-plugin": "^2.6.1",
    "null-loader": "^4.0.1",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "serverless": "^3.23.0",
    "serverless-offline": "~11.1.3",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-s3-deploy": "^0.10.1",
    "serverless-webpack": "^5.10.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.8.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-stats-plugin": "^1.1.0"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always"
  }
